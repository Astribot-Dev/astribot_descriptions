<mujoco>

    <sensor>
<!--        <touch name="astribot_chassis_wheel_force_left" site="astribot_chassis_wheel_force_left" />-->
        <!-- <force name="astribot_chassis_front_left_force" site="astribot_chassis_front_left_site" /> -->
